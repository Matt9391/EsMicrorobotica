import numpy as np
import matplotlib.pyplot as plt
import pandas as pd


data = pd.read_csv("./penguins_size.csv")
data # oggetto di tipo dataframe


data.dropna(inplace = True) # NON andrebbe fatta cos√¨
data


dataArray = data[["culmen_length_mm","culmen_depth_mm","flipper_length_mm","body_mass_g"]].to_numpy()
species = data["species"].to_numpy()


dataArray.shape
dataArray.dtype





x = dataArray[:, 2]
y = dataArray[:, 3]
z = dataArray[:, 1]

fig, axs = plt.subplots(nrows=1, ncols=2, figsize = (8,4))

axs[0].scatter(x,y, edgecolor = "k", alpha = 0.7)
axs[0].set_xlabel("flipper length [mm]")
